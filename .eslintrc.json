{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {
      "semi": "off",
      "react/react-in-jsx-scope": "off",
      "space-before-function-paren": "off"
    }
}
